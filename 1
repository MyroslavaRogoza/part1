2
n = 1
n = input("naturalne chislo ")
i = 1
resultat = 0
while i < int(n):
    tryVstepeniN = 3
    j = 1
    while j<i:
        tryVstepeniN=tryVstepeniN*3
        j=j+1
    nfact = 1
    j2 = 1
    while j2<i:
        nfact=nfact*j2
        j2=j2+1
    nVstepeniN = i
    j3 = 1
    while j3<i:
        nVstepeniN=nVstepeniN*i
        j3=j3+1
    resultat=resultat+(tryVstepeniN*nfact)/nVstepeniN
    i=i+1
    print("Resultat",resultat)
print(resultat)
