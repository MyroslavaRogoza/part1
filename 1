prov = False
while(prov!=True):
    a=int(input("te scho dilytsya "))
    b=int(input("te na scho dilytsya "))
    if(b==0):
        print("na nol dilyty ne mozhna")
    else:
        prov=True
c=0
prov2 = int(a)%int(b)
if (prov2==0):
    while a!=0:
        c=c+1
        a=a-b
    print('Result: ',c)
else:
    while a > b:
        c = c + 1
        a = a - b
    print('Result: ', c,'Z ostatkom', a)
